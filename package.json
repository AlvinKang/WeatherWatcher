  "bugs": {
    "url": "https://github.com/AlvinKang/WeatherWatcher/issues"
  },
  "homepage": "https://github.com/AlvinKang/WeatherWatcher#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-moment": "^0.7.6",
    "react-router-dom": "^4.3.1",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
